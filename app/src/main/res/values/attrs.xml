<resources>
    <declare-styleable name="ButtonEditText">
        <attr name="activated" format="boolean"/>
        <attr name="increment" format="float"/>
        <attr name="default_number" format="float"/>
        <attr name="mode" format="enum">
            <enum name="integer" value="0"/>
            <enum name="decimal" value="1"/>
            <enum name="time" value="2"/>
        </attr>
        <attr name="always_positive" format="boolean"/>
        <attr name="hint" format="string"/>
    </declare-styleable>
</resources>
